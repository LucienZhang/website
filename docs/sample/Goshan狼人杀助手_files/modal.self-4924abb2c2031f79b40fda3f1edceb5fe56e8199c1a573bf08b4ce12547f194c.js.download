(function() {
  var ref;

  this.Wolf = (ref = this.Wolf) != null ? ref : {};

  this.Wolf.modal = {
    alert: function(data) {
      var msg;
      msg = Wolf.Trans.Panel.alert_message_trans[data.msg];
      if (msg) {
        msg = Wolf.Trans.insert_params(msg, data);
      } else {
        msg = data.msg;
      }
      return BootstrapDialog.alert(msg);
    },
    dialog: function(data) {
      var buttons;
      if (Wolf.panel.skillParams.action !== 'none') {
        return;
      }
      buttons = [
        {
          label: "确认",
          cssClass: 'btn-success',
          action: (function(_this) {
            return function(dialog, e) {
              App.game["do"]('confirm_skill');
              return dialog.close();
            };
          })(this)
        }
      ];
      if (data.retry) {
        buttons.push({
          label: "重新操作",
          cssClass: 'btn-default',
          action: (function(_this) {
            return function(dialog, e) {
              App.game["do"]('prepare_skill');
              return dialog.close();
            };
          })(this)
        });
      }
      return BootstrapDialog.show({
        closable: false,
        message: Wolf.Trans.insert_params(Wolf.Trans.Panel.dialog_message_trans[data.msg], data),
        buttons: buttons
      });
    },
    display_role: function(data) {
      $('#check-role-dialog .role').text(Wolf.Trans.Roles[data.role]);
      $('#check-role-dialog .role-card').addClass('hidden');
      $("#check-role-dialog .role-" + data.role).removeClass('hidden');
      return $('#check-role-dialog').modal('show');
    },
    display_bid_info: function(data) {
      var price, ref1, role;
      $('#bid-info-modal .coin-balance-val').text(data.coin);
      $('#bid-info-modal .current-bid').text("");
      ref1 = data.bid;
      for (role in ref1) {
        price = ref1[role];
        $('#bid-info-modal .current-bid').append("<span>" + Wolf.Trans.Roles[role] + ": " + price + "</span>");
      }
      return $('#bid-info-modal').modal('show');
    }
  };

}).call(this);

(function() {
  var ref;

  this.Wolf = (ref = this.Wolf) != null ? ref : {};

  this.Wolf.Utils = (function() {
    function Utils() {}

    Utils.varIsNull = function(p) {
      return typeof p === 'undefined' || p === null;
    };

    Utils.arrayIsEmpty = function(arr) {
      return typeof arr === 'undefined' || arr === null || arr.length === 0;
    };

    return Utils;

  })();

}).call(this);
